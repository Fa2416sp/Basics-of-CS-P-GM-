CC = gcc
CFLAGS = -Wall -Wextra -std=c11

all: primes

primes: primes.o
	$(CC) $(CFLAGS) -o primes primes.o

primes.o: primes.c
	$(CC) $(CFLAGS) -c primes.c

clean:
	rm -f *.o primeslychrel: lychrel.o
	gcc -o lychrel  lychrel.o

lychrel.o: lychrel.c
	gcc -g -O0 -c -Wall -Wextra lychrel.c
clean:
	rm task3 *.o
indent:
	indent -kr -nut lychrel.c

